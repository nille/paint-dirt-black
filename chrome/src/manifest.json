{   
   "content_scripts": [ {
      "js": [ "js/jquery.min.js", "js/content.js", "js/match.js" ], 
      "matches": [ "http://*/*", "https://*/*"  ]
   } ],
   "content_security_policy": "script-src 'self'; object-src 'self'",
   "description": "A browser plugin that removes hostile and offensive language throughout the web. Any unsuitable language will simply appear blacked out, making the WWW a better place.", 
   "manifest_version": 2,
   "name": "Paint dirt black",
   "icons": { 	"16": "icon.jpg",
				"48": "icon.jpg",
				"128": "icon.jpg" },   
   "version": "1.0",
   "web_accessible_resources": [
        "css/font/webfonts/BLOKKRegular.svg",
        "css/font/webfonts/BLOKKRegular.woff",
        "css/font/webfonts/BLOKKRegular.eot" 
    ]
}
